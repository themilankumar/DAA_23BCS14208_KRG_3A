#include <iostream>
#include <vector>
using namespace std;

int subarraySum(const vector<int>& nums, int k) {
    int count = 0;
    int n = nums.size();
    for (int i = 0; i < n; ++i) {
        long long current_sum = 0;
        for (int j = i; j < n; ++j) {
            current_sum += nums[j];
            if (current_sum == k) {
                count++;
            }
        }
    }
    return count;
}

int main() {
    vector<int> nums = {1, 2, 3, -3, 1, 1, 1};
    int k =6;
    cout << "Number of subarrays with sum " << k << " is: " 
              << subarraySum(nums, k) <<endl;
    return 0;
}
